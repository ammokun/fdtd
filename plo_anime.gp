#-------------------------------------------------------------------------------
# gnuplotの設定
#-------------------------------------------------------------------------------
reset


set pm3d 
set pm3d map
set ticslevel 0
set cbrange[-1E5:1E5]
set palette defined (-1E5 "blue", 0 "white", 1E5 "red")
set term gif animate     # 出力をgifアニメに設定
set output "animation.gif" # 出力ファイル名の設定

#-------------------------------------------------------------------------------
# 変数の設定
#-------------------------------------------------------------------------------
n0 = 1    # ループ変数の初期値
n1 = 100  # ループ変数の最大値
dn = 1    # ループ変数の増加間隔

#-------------------------------------------------------------------------------
# ループの開始
#-------------------------------------------------------------------------------
load "plo_anime.plt" 